{
    "name": "springimport/swagger-magento2-client",
    "description": "Rest API client for Magento 2 generated by swagger-codegen.",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "http://swagger.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "satooshi/php-coveralls": "~1.0",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "SpringImport\\Swagger\\Magento2\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "SpringImport\\Swagger\\Magento2\\Client\\" : "test/" }
    }
}
